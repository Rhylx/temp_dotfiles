partial alphanumeric_keys
xkb_symbols "basic" {
    name[Group1]="French (AZERTY)";
    name[Group2]="French (AZERTY)";

    include "level3(ralt_switch)"
    //include "group(lswitch)"

// Rhylx  AZERTY-Keyboard layout
// Author : 2018, Rhylx
// Based on the fr(azerty) layout

// LAYOUT OVERVIEW
//  ____ ____ ____ ____ ____ ____ ____ ____ ____ ____ ____ ____ ____ _______
// |    |    |    |    |    |    |    |    |    |    |    |    |    | <--   |
// | ²  | 1  | 2  | 3  | 4  | 5  | 6  | 7  | 8  | 9  | 0  | +  | =  |       |
//  ========================================================================
// | |<-  | Q  | W  | E  | R  | T  | Y  | U  | I  | O  | P  | `  | ¨  |   , |
// |  ->| | q  | w  | e ç| r  | t  | y  | u  | i  | o  | p  | ´  | ^  | <-' |
//  ===================================================================¬    |
// |       | A  | S  | D  | F  | G  | H  | J  | K  | L  |?  | \  | µ  |    |
// | ESC   | a @| s ~| d "| f #| g {| h [| j (| k -| l ||, &| %  | *  |    |
//  ========================================================================
// | ^   | <  | Z  | X  | C  | V  | B  | N  | M  | ;  | /  | §  |     ^     |
// | |   | >  | z \| x '| c $| v }| b ]| n )| m _| .  | :  | !  |     |     |
//  ========================================================================
// |      |      |      |                       |       |      |     |      |
// | Ctrl | Super| Alt  | Space    Nobreakspace | AltGr | Super|Menu | Ctrl |
//  ¯¯¯¯¯¯ ¯¯¯¯¯¯ ¯¯¯¯¯¯ ¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯¯ ¯¯¯¯¯¯¯ ¯¯¯¯¯¯ ¯¯¯¯¯ ¯¯¯¯¯¯

    // First row
    key <TLDE>	{ [	twosuperior 			] };
    key <AE01>	{ [	1		 		] };
    key <AE02>  { [	2,		2		] };
    key <AE03>	{ [	3,		3		] };
    key <AE04>	{ [	4,		4		] };
    key <AE05>	{ [	5,		5		] };
    key <AE06>	{ [	6,		6		] };
    key <AE07>	{ [	7,		7		] };
    key <AE08>	{ [	8,	 	8		] };
    key <AE09>	{ [	9,	 	9		] };
    key <AE10>	{ [	0,		0		] };
    key <AE11>	{ [	plus,		degree		] };
    key <AE12>	{ [	equal,		plus		] };

    // Second row
    key <AD01>	{ [	q,		Q             			] };
    key <AD02>	{ [	w,		W				] };
    key <AD03>	{ [	e,		E,		ccedilla	] };
    key <AD04>	{ [	r,		R				] };
    key <AD05>	{ [	t,		T				] };
    key <AD06>	{ [	y,		Y				] };
    key <AD07>	{ [	u,		U				] };
    key <AD08>	{ [	i,		I				] };
    key <AD09>	{ [	o,		O				] };
    key <AD10>	{ [	p,		P				] };
    key <AD11>	{ [	dead_acute,	dead_grave,	plus		] };
    key <AD12>	{ [	dead_circumflex,dead_diaeresis,	equal		] };

    // Third row
    key <AC01>	{ [	a,		A,		at		] };
    key <AC02>	{ [	s,		S,		asciitilde	] };
    key <AC03>	{ [	d,		D,		quotedbl	] };
    key <AC04>	{ [	f,		F,		numbersign	] };
    key <AC05>	{ [	g,		G,		braceleft	] };
    key <AC06>	{ [	h,		H,		bracketleft	] };
    key <AC07>	{ [	j,		J,		parenleft	] };
    key <AC08>	{ [	k,		K,		minus		] };
    key <AC09>	{ [	l,		L,		bar		] };
    key <AC10>	{ [	comma,		question,	ampersand	] };
    key <AC11>	{ [	percent,	backslash			] };
    key <BKSL>  { [	asterisk,	mu				] };

    // Fourth row
    key <LSGT>	{ [	greater,	less				] };
    key <AB01>	{ [	z,		Z,		backslash	] };
    key <AB02>	{ [	x,		X,		apostrophe	] };
    key <AB03>	{ [	c,		C,		dollar		] };
    key <AB04>	{ [	v,		V,		braceright	] };
    key <AB05>  { [	b,		B,		bracketright	] };
    key <AB06>	{ [	n,		N,		parenright	] };
    key <AB07>	{ [	m,		M,		underscore	] };
    key <AB08>	{ [ 	period,		semicolon			] };
    key <AB09>	{ [ 	colon,		slash				] };
    key <AB10>	{ [	exclam,		section				] };
};
